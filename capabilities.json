{
    "dataRoles": [
        {
            "displayName": "Course Title",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Score Data",
            "name": "score",
            "kind": "Measure"
        },
        {
            "displayName": "Request Score Data",
            "name": "reqscore",
            "kind": "Measure"
        },
        {
            "displayName": "Full Score Data",
            "name": "fullscore",
            "kind": "Measure"
        },
        {
            "displayName": "Main course check",
            "name": "maincategory",
            "kind": "Measure"
        }
    ],
    "objects": {
        "dataPoint": {
            "properties": {
                "defaultColor": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showAllDataPoints": {
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fillRule": {
                    "type": {
                        "fill": {}
                    }
                },
                 "fontSize": {
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "score"
                            }
                        },
                        {
                            "bind": {
                                "to": "reqscore"
                            }
                        },
                        {
                            "bind": {
                                "to": "fullscore"
                            }
                        },
                        {
                            "bind": {
                                "to": "maincategory"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "privileges": []
}